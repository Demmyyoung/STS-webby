{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "product"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "price": {
      "type": "float"
    },
    "description": {
      "type": "blocks"
    },
    "productimage": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "product_varients": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::product-varient.product-varient",
      "mappedBy": "product"
    }
  }
}
