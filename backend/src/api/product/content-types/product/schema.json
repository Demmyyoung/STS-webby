{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "product"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "price": {
      "type": "float"
    },
    "description": {
      "type": "blocks"
    },
    "productimage": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "quantity": {
      "type": "integer"
    },
    "size": {
      "type": "enumeration",
      "enum": [
        "xs",
        "s",
        "m",
        "l",
        "xl",
        "xxl"
      ]
    }
  }
}
